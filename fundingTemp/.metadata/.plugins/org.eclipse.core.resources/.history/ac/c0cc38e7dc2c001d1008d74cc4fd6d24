create table profunding(
title varChar(100),
nickname varChar(100),
price int,
personnel int
)

create table account (
	id int not null auto_increment primary key,
	email varChar(100),
	pw varchar(100),
	name varchar(100),
	nickname varchar(100),
	addr varchar(100),
	phnum varchar(100),
	bankacc varchar(100),
	company varchar(100)
);
create table community (
	cid int not null auto_increment primary key,
	title varchar(100),
	nickname varchar(100),
	contentname varchar(100),
	content varchar(100),
	reg_date datetime,
	gid int 
);
create table project (
	pid int not null auto_increment primary key,
	id int ,
	title varchar(100) ,
	content varchar(100),
	wname varchar(100),
	reg_date datetime,
	detail varchar(100),
	cnt int,
	cat varchar(100)
);